{
	"compilerOptions": {
		"target": "es2022",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"module": "es2022",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js"
	},
	"include": ["src"]
}
